integer function fibonacci (integer n) {
	integer x is 1,y is 1.
	if n isequal 0 or n isequal 1 then {
		return 1.
	} 
	
	for i from 0 to n {
		integer temp is x plus y.
		y is x.
		x is temp.
	}
	return x.
}


